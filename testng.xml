<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Rest Assured Suite">
    <test name="Reqres multiple tests" enabled="false">
        <classes>
            <class name="io.github.mfaisalkhatri.ReqresTests">
                <methods>
                    <include name="testGetUsers"/>
                    <include name="testPostUser"/>
                    <include name="testPostUser_1"/>
                    <include name="testUpdateUser"/>
                    <include name="testUpdatePartialUser"/>
                    <include name="testDeleteUser"/>
                    <include name="testGetUsersMultiAssert"/>
                    <include name="testGetUserJsonExample"/>
                    <include name="testNumberAssertions"/>
                    <include name="testStringAssertions"/>
                    <include name="testHasKeyAssertion"/>
                    <include name="testNotAssertions"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="Restful Booker tests">
        <classes>
            <class name="io.github.mfaisalkhatri.RestfulBookerTests">
                <methods>
                    <include name="testCreateBooking"/>
                    <include name="testGetBooking"/>
                </methods>
            </class>
        </classes>
